# create and print a linked list using pairs

# Output:
# list = {1, 2, 4, 11}
#

# Program:

begin
    p = newpair(11, null) ;
    q = newpair(4, p) ;
    r = newpair(2, q) ;
    s = newpair(1, r) ;
    print "list = {" ;
    x = s ;
    y = snd x ;
    f = 0;
    while y != null do
      f = fst x ;
      print f ;
      print ", " ;
      x = y ;
      y = snd x
    done ;
    f = fst x ;
    print f ;
    println "}"
end